# 微信机器人配置
# 请复制此文件为 .env 并填写实际配置

# 微信 puppet（留空使用默认）
WECHATY_PUPPET=

# 管理员微信号（用于接收登录通知）
ADMIN_WECHAT_ID=

# 推送目标用户列表（填写Wechat ID，多个用逗号分隔）
TARGET_USERS=

# 新闻源开关
ENABLE_MIIT=true
ENABLE_UNICOM=true
ENABLE_ANTI_FRAUD=true

# 推送时间（24小时制）
PUSH_HOUR=9
PUSH_MINUTE=0

# 每日推送条数
NEWS_COUNT=5

# 日志级别
LOG_LEVEL=info